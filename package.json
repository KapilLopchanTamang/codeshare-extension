{
  "name": "sendcode",
  "displayName": "sendcode",
  "publisher": "AbijeetRaut",
  "description": "",
  "icon": "media/codeshare.png",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/abijeetraut1/codeshare-extension.git"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
